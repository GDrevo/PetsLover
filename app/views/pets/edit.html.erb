<%= simple_form_ for @pet do |f| %>
  <%= f.label :specie %>
  <%= f.text_field :specie %>
  <%= f.label :breed %>
  <%= f.text_field :breed %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.submit %>
<% end %>
